#include <stdio.h>

int main() {
    int grade;
    printf("Enter your grade (0-100): ");
    scanf("%d", &grade);
    if(grade < 0 || grade > 100) {
        printf("Invalid grade! Please enter a value between 0-100.\n");
        return 1;
    }
    int caseValue = grade / 10;
    printf("Grade: ");
    switch(caseValue) {
        case 10:
        case 9:
            printf("A\n");
            break;
        case 8:
            printf("B\n");
            break;
        case 7:
            printf("C\n");
            break;
        default:
            printf("Failed\n");
            break;
    }
    return 0;
}
